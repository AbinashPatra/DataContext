{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Properties": {
    "AppName": "DataContextTestWeb",
    "DataProtectionFile": "DataProtection-Keys-DataContextTestWeb"
  },
  "RedisConfiguration": {
    "Dns": "127.0.0.1",
    "Port": "6379",
    "UseSsl": false, //True if the instance is configured for ssl only.
    "ConnectRetry": 3,
    "ConnectTimeout": 5000,
    "SyncTimeout": 5000,
    "DefaultDatabase": 5, //Index of the database. Should be one dedicated for hpg web in order to avoid any unnecessary interruption
    "AbortOnConnectFail": false,
    "Password": "" //Provide, if the instance is password protected
  }
}
